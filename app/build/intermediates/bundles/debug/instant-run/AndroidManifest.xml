<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gsa.gsa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="gsa.gsa.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="gsa.gsa.Administracion"
            android:label="@string/title_activity_administracion" />
        <activity
            android:name="gsa.gsa.AgregarUsuariosSistemaActivity"
            android:label="Agregar Usuarios del Sistema" />
        <activity
            android:name="gsa.gsa.Clientes"
            android:label="Clientes" />
        <activity
            android:name="gsa.gsa.AgregarClientes"
            android:label="Agregar Clientes" />
        <activity
            android:name="gsa.gsa.Gestion"
            android:label="@string/title_activity_gestion" />
        <activity
            android:name="gsa.Interfaz.FragmentoGestion"
            android:label="@string/title_activity_gestion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="gsa.gsa.Trabajadores"
            android:label="Trabajadores" />
        <activity
            android:name="gsa.gsa.AgregarTrabajadores"
            android:label="@string/title_activity_agregar_trabajadores" />
        <activity
            android:name="gsa.gsa.Servicios"
            android:label="Servicios" />
        <activity
            android:name="gsa.gsa.AgregarServicios"
            android:label="Agregar Servicios" />
        <activity
            android:name="gsa.gsa.Contratos"
            android:label="Contratos" />
        <activity android:name="gsa.gsa.AgregarContrato" />
    </application>

</manifest>